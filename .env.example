# AgenticVoice.net Environment Variables
# Copy this file to .env.local and fill in your actual values

# ===== NEXTAUTH CONFIGURATION =====
NEXTAUTH_SECRET="your-secret-key-here-generate-with-openssl-rand-base64-32"
NEXTAUTH_URL="http://localhost:3000"

# ===== DATABASE CONFIGURATION =====
# Get from MongoDB Atlas: https://cloud.mongodb.com/
MONGODB_URI="mongodb+srv://username:password@cluster.mongodb.net/agenticvoice?retryWrites=true&w=majority"

# ===== GOOGLE OAUTH CONFIGURATION =====
# Get from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_ID="your-google-client-id.apps.googleusercontent.com"
GOOGLE_SECRET="your-google-client-secret"

# ===== EMAIL CONFIGURATION =====
# Get from Resend: https://resend.com/
RESEND_API_KEY="re_your-resend-api-key"

# ===== VAPI.AI CONFIGURATION (Optional for Phase 1) =====
VAPI_API_KEY="your-vapi-api-key"
VAPI_PRIVATE_KEY=
VAPI_WEBHOOK_SECRET="your-vapi-webhook-secret"

# ===== STRIPE CONFIGURATION =====
# Get from Stripe Dashboard: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_PUBLIC_KEY="pk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# Get from Stripe Products: https://dashboard.stripe.com/products
STRIPE_PRICE_ID_PAYPERUSE="price_..."
STRIPE_PRICE_ID_ESSENTIAL="price_..."
STRIPE_PRICE_ID_STARTER="price_..."
STRIPE_PRICE_ID_PROFESSIONAL="price_..."
STRIPE_PRICE_ID_BUSINESS="price_..."
STRIPE_PRICE_ID_ENTERPRISE="price_..."

# ===== N8N CONFIGURATION (Optional for Phase 1) =====
N8N_WEBHOOK_URL="https://your-n8n-instance.com/webhook"
N8N_API_KEY="your-n8n-api-key"
